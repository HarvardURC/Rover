  <world name="default">

    <!-- Ground Plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

   <model name="car">
    <pose>1 -1.5 7.5 0 0 0</pose>
    <link name='chassis'>
        <gravity>0</gravity>
          <pose>0 0 .4 0 0 0</pose>
          <inertial>
            <inertia>
              <ixx>1</ixx>
              <ixy>0</ixy>
              <ixz>0</ixz>
              <iyy>1</iyy>
              <iyz>0</iyz>
              <izz>1</izz>
            </inertia>
            <mass>10.0</mass>
          </inertial>
          <collision name='collision'>
            <geometry>
              <box>
                <size>2 1.5 .2</size>
              </box>
            </geometry>
          </collision>

          <visual name='visual'>
            <geometry>
              <box>
                <size>2 1.5 .2</size>
              </box>
            </geometry>
            <material>
              <ambient>1 0 0 1</ambient>
              <shader type='vertex'>
                <normal_map>__default__</normal_map>
              </shader>
            </material>
          </visual>
      </link>

      <link name="front_indicator">
        <pose>1 0 0.6 0 0 0</pose>
          <geometry>
              <sphere>
              <radius>.06</radius>
            </sphere>
          </geometry>
        <visual name="visual">
          <geometry>
              <sphere>
              <radius>.06</radius>
            </sphere>
          </geometry>
          <material>Gazebo/Blue</material>
        </visual>
      </link>

      <link name="left_front_wheel">
        <pose>1 .85 .16 0 0 3.1414</pose>
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>2.0</mass>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.7</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.1</radius>
              <length>.7</length>
            </cylinder>
          </geometry>
        </visual>
      </link>

