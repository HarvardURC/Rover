        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </visual>
      </link>


      <link name="right_back_wheel">
        <pose>-.5 -.27 .1 0 0 1.5707</pose>
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
