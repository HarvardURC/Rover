      <joint type="fixed" name="indicator_joint">
        <pose>0 0 0 0 0 0</pose>
        <child>front_indicator</child>
        <parent>chassis</parent>
      </joint>

      <plugin name="legged" filename="liblegged.so"/>
    </model>
</world>
</sdf>