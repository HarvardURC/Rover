<?xml version="1.0"?> 
<sdf version="1.4">
  <world name="default">

    <!-- Ground Plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <model name="car">
    <pose>0.0 0.0 .3 0 0 0</pose>
    <link name='chassis'>
        <gravity>0</gravity>
          <pose>0 0 .2 0 0 0</pose>
          <inertial>
            <inertia>
              <ixx>1</ixx>
              <ixy>0</ixy>
              <ixz>0</ixz>
              <iyy>1</iyy>
              <iyz>0</iyz>
              <izz>1</izz>
            </inertia>
            <mass>5.0</mass>
          </inertial>
          <collision name='collision'>
            <geometry>
              <box>
                <size>1 .5 .1</size>
              </box>
            </geometry>
          </collision>

          <visual name='visual'>
            <geometry>
              <box>
                <size>1 .5 .1</size>
              </box>
            </geometry>
          </visual>
      </link>

      <link name="front_indicator">
        <pose>.5 0 0.3 0 0 0</pose>
          <geometry>
              <sphere>
              <radius>.03</radius>
            </sphere>
          </geometry>
        <visual name="visual">
          <geometry>
              <sphere>
              <radius>.03</radius>
            </sphere>
          </geometry>
          <material>Gazebo/Blue</material>
        </visual>
      </link>




      <link name="left_front_wheel">
        <pose>.5 .27 .1 0 0 1.5707</pose>
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>1.0</mass>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </visual>
      </link>


      <link name="right_front_wheel">
        <pose>.5 -.27 .1 0 0 1.5707</pose>
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>1.0</mass>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </visual>
      </link>


      <link name="left_middle_wheel">
        <pose>0 .27 .1 0 0 1.5707</pose>
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>1.0</mass>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </visual>
      </link>


      <link name="right_middle_wheel">
        <pose>0 -.27 .1 0 0 1.5707</pose>
        <inertial>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
          <mass>1.0</mass>
        </inertial>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>.02</radius>
              <length>.2</length>
            </cylinder>
          </geometry>
        </visual>
      </link>


      <link name="left_back_wheel">
        <pose>-.5 .27 .1 0 0 1.5707</pose>
        <inertial>
